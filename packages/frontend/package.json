{
  "name": "@intelligenai/frontend",
  "version": "1.0.0",
  "description": "React-based frontend for IntelGenAI",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "test": "react-scripts test --watchAll=false",
    "test:unit": "react-scripts test --testPathPattern=unit --watchAll=false",
    "test:pbt": "react-scripts test --testPathPattern=property --watchAll=false",
    "lint": "eslint src/**/*.{ts,tsx}",
    "perf:audit": "lighthouse http://localhost:3000 --output=json --output-path=./performance-audit.json",
    "perf:monitor": "node -e \"console.log('Performance monitoring enabled in development mode')\"",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@intelligenai/shared": "^1.0.0",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.6.2",
    "framer-motion": "^12.23.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^10.1.8",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^10.1.8",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/preset-create-react-app": "^10.1.8",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^10.1.8",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-plugin-storybook": "^10.1.8",
    "fast-check": "^3.15.0",
    "lighthouse": "^11.4.0",
    "prop-types": "^15.8.1",
    "react-scripts": "5.0.1",
    "storybook": "^8.6.14",
    "typescript": "^4.9.5",
    "webpack": "^5.103.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  }
}